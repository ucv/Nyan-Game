/**
 * Permet de gerer toutes les caisses du jeu
 */
define([
],
function (
) {
	var CrateManager = function () {
		this.list = [];
	}


	CrateManager.prototype.update = function () {
		for (var i = 0; i < this.list.length; i++) {
			this.list[i].update();
		};
	}


	return new CrateManager();
});